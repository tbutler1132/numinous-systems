{
  "name": "xenoscript",
  "displayName": "XenoScript",
  "description": "Syntax highlighting for XenoScript (.xeno) files",
  "version": "0.1.0",
  "publisher": "vital-systems",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "xenoscript",
        "aliases": ["XenoScript", "xeno"],
        "extensions": [".xeno"],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/xeno-light.svg",
          "dark": "./icons/xeno.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "xenoscript",
        "scopeName": "source.xenoscript",
        "path": "./syntaxes/xenoscript.tmLanguage.json"
      }
    ]
  }
}
